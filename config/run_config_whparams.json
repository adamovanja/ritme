{
  "experiment_tag": "run_config",
  "fully_reproducible": true,
  "group_by_column": "host_id",
  "ls_model_types": [
    "linreg",
    "trac",
    "xgb",
    "nn_reg",
    "nn_class",
    "nn_corn",
    "rf"
  ],
  "max_cuncurrent_trials": 5,
  "model_hyperparameters": {
    "linreg": {
      "alpha": {
        "log": true,
        "max": 100,
        "min": 1e-05
      },
      "l1_ratio": {
        "max": 1,
        "min": 0,
        "step": 0.1
      }
    },
    "nn_all_types": {
      "batch_size": [
        32,
        64,
        128,
        256
      ],
      "dropout_rate": {
        "max": 0.5,
        "min": 0.0,
        "step": 0.05
      },
      "early_stopping_min_delta": {
        "log": true,
        "max": 0.01,
        "min": 1e-05
      },
      "early_stopping_patience": {
        "max": 10,
        "min": 2,
        "step": 1
      },
      "epochs": [
        10,
        50,
        100,
        200
      ],
      "learning_rate": [
        0.01,
        0.005,
        0.001,
        0.0005,
        0.0001,
        5e-05,
        1e-05
      ],
      "n_hidden_layers": {
        "max": 30,
        "min": 1,
        "step": 5
      },
      "n_units_hl": [
        32,
        64,
        128,
        256,
        512
      ],
      "weight_decay": {
        "log": true,
        "max": 0.01,
        "min": 1e-06
      }
    },
    "rf": {
      "bootstrap": [
        true,
        false
      ],
      "max_depth": [
        4,
        8,
        16,
        32,
        null
      ],
      "max_features": [
        null,
        "sqrt",
        "log2",
        0.1,
        0.2,
        0.3,
        0.5
      ],
      "min_impurity_decrease": {
        "log": false,
        "max": 0.5,
        "min": 0.0
      },
      "min_samples_leaf": {
        "log": true,
        "max": 0.1,
        "min": 0.001
      },
      "min_samples_split": {
        "log": true,
        "max": 0.1,
        "min": 0.001
      },
      "min_weight_fraction_leaf": {
        "log": false,
        "max": 0.01,
        "min": 0.0
      },
      "n_estimators": {
        "max": 200,
        "min": 40,
        "step": 20
      }
    },
    "trac": {
      "lambda": {
        "log": true,
        "max": 1,
        "min": 0.001
      }
    },
    "xgb": {
      "colsample_bytree": {
        "max": 1.0,
        "min": 0.3
      },
      "eta": {
        "max": 0.3,
        "min": 0.01
      },
      "gamma": {
        "max": 5.0,
        "min": 0.0,
        "step": 0.1
      },
      "max_depth": {
        "max": 10,
        "min": 2
      },
      "min_child_weight": {
        "max": 4,
        "min": 0
      },
      "num_parallel_tree": {
        "max": 3,
        "min": 1,
        "step": 1
      },
      "reg_alpha": {
        "log": true,
        "max": 1.0,
        "min": 1e-10
      },
      "reg_lambda": {
        "log": true,
        "max": 1.0,
        "min": 1e-10
      },
      "subsample": {
        "max": 1.0,
        "min": 0.7
      }
    }
  },
  "num_trials": 2,
  "optuna_searchspace_sampler": "TPESampler",
  "seed_data": 12,
  "seed_model": 12,
  "target": "age_months",
  "tracking_uri": "mlruns"
}
